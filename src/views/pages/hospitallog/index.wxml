
<view class="log-list">
  <view class="log-title">
    <view class="title-name">
      <text>研究中心</text>
    </view>
    <view class="title-time">
      <text>类型及时间</text>
    </view>
  </view>
  <view
    class="log-item"
    data-item="{{ item }}"
    wx:for="{{ logList }}"
    wx:key="index">
    <view class="left">
      <view class="iconfont {{index%2 === 0?'icon-add':'icon-change'}}"></view>
      <view class="text">
        <view>
          <text>山东省-青岛市</text>
        </view>
        <view>
          <text>青岛市立医院</text>
        </view>
      </view>
    </view>
    <view class="text">
      <view>
        <text>待审核 变为 审核失败</text>
      </view>
      <view>
        <text>{{item.time}}</text>
      </view>
    </view>
  </view>
</view>
