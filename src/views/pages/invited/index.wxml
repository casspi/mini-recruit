<wow-skeleton
  bindrefresh="pagingRefresh"
  error="{{ error }}"
  wx:if="{{ pagingTotal <= 0 }}"
  is-loading="{{ pagingTotal === -1 }}"
>

</wow-skeleton>
<view class="invited-list" wx:else>
  <block
    wx:for="{{ pagingData  }}"
    wx:key="arrIndex"
    wx:for-item="arrData"
    wx:for-index="arrIndex">
    <view
      class="invited-item {{ item.experimentStage == 2 && 'no-read'}}"
      bindtap="jumpPageOrFireFn"
      data-url="patient_index"
      wx:for="{{ arrData }}"
      wx:key="id">
      <view class="invited-item-top">
        <view class="patient">
          <view class="name">刘备</view>
          <view class="status">
            <text>医学审核中</text>
          </view>
        </view>
        <view class="time">
          <tex>03-18 16:05</tex>
        </view>
      </view>

    </view>
  </block>
  <wow-end
    wx:if="{{ pagingTotal > 0}}"
    is-loading="{{ pagingTotal > pagingNumTotal }}"
  ></wow-end>
</view>
